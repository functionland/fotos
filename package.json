    "@types/jest": "26.0.19",
    "@types/react": "^18.2.6",
    "@types/react-native": "^0.73.0",
    "@types/react-test-renderer": "18.0.7",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
